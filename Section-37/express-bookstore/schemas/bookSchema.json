{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "book"
    ],
    "properties": {
        "book": {
            "type": "object",
            "default": {},
            "title": "The book Schema",
            "required": [
                "isbn",
                "amazon_url",
                "author",
                "language",
                "pages",
                "publisher",
                "title",
                "year"
            ],
            "properties": {
                "isbn": {
                    "type": "string",
                    "default": "",
                    "title": "The isbn Schema",
                    "examples": [
                        "12345"
                    ]
                },
                "amazon_url": {
                    "type": "string",
                    "default": "",
                    "title": "The amazon_url Schema",
                    "examples": [
                        "www.book.com"
                    ]
                },
                "author": {
                    "type": "string",
                    "default": "",
                    "title": "The author Schema",
                    "examples": [
                        "Aaron"
                    ]
                },
                "language": {
                    "type": "string",
                    "default": "",
                    "title": "The language Schema",
                    "examples": [
                        "English"
                    ]
                },
                "pages": {
                    "type": "integer",
                    "default": 0,
                    "title": "The pages Schema",
                    "examples": [
                        325
                    ]
                },
                "publisher": {
                    "type": "string",
                    "default": "",
                    "title": "The publisher Schema",
                    "examples": [
                        "Some Publisher"
                    ]
                },
                "title": {
                    "type": "string",
                    "default": "",
                    "title": "The title Schema",
                    "examples": [
                        "Some Title"
                    ]
                },
                "year": {
                    "type": "integer",
                    "default": 0,
                    "title": "The year Schema",
                    "examples": [
                        2023
                    ]
                }
            },
            "examples": [{
                "isbn": "12345",
                "amazon_url": "www.book.com",
                "author": "Aaron",
                "language": "English",
                "pages": 325,
                "publisher": "Some Publisher",
                "title": "Some Title",
                "year": 2023
            }]
        }
    },
    "examples": [{
        "book": {
            "isbn": "12345",
            "amazon_url": "www.book.com",
            "author": "Aaron",
            "language": "English",
            "pages": 325,
            "publisher": "Some Publisher",
            "title": "Some Title",
            "year": 2023
        }
    }]
}